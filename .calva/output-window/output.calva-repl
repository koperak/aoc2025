(defn dial-path
    [start dir steps]
    (let [start (mod start 100)
          sign  (case dir
                  "R"  1
                  "L" -1)]
      ;; reductions robi: start -> (start+sign) -> (start+2*sign) -> ...
      ;; My bierzemy TYLKO kolejne kroki, czyli drop 1.
      (->> (reductions
            (fn [pos _]
              (mod (+ pos sign) 100))
            start
            (range steps))
           (drop 1)   ;; usuwamy pozycjƒô startowƒÖ
           vec)))
